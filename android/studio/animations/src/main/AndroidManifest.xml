<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joyous.animations" >

    <application
        android:allowBackup="true"
        android:label="@string/app_name" >
        <activity
            android:name=".AnimationsActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="animations"
            android:value="com.joyous.animations.ModuleEntry" />

        <activity
            android:name=".FrameAnimationActivity"
            android:label="@string/title_activity_frame_animation" >
        </activity>
    </application>

</manifest>
